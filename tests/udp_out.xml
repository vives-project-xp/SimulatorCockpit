<?xml version="1.0"?>
<PropertyList>
  <generic>
    <output>
      <line_separator>newline</line_separator>
      <var_separator>space</var_separator>

      <chunk>
	<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
        <type>float</type>
        <format>AIRSPEED=%6.2f</format>
      </chunk>

      <chunk>
        <node>/orientation/heading-deg</node>
        <type>float</type>
        <format>HEADING=%6.2f</format>
      </chunk>

    </output>
  </generic>
</PropertyList>
